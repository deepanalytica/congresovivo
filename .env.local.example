# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Upstash Redis (for caching)
UPSTASH_REDIS_REST_URL=your_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# Open Data API Base URLs
OPENDATA_CAMARA_BASE_URL=https://opendata.camara.cl
OPENDATA_SENADO_BASE_URL=https://tramitacion.senado.cl/wspublico
BCN_BASE_URL=https://www.bcn.cl/leychile

# Feature Flags (for incremental development)
NEXT_PUBLIC_ENABLE_REAL_DATA=false
NEXT_PUBLIC_ENABLE_SEARCH=true
NEXT_PUBLIC_ENABLE_PRO_FEATURES=false
NEXT_PUBLIC_ENABLE_PARLAMENTARIAN_MAP=true

# Development
NODE_ENV=development
