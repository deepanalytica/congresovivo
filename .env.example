# IMPORTANT: Copy this file to .env.local and fill in your values
# .env.local is gitignored and will not be committed

# ============================================
# Supabase (Required)
# ============================================
# Get these from: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ============================================
# ETL Configuration (Optional)
# ============================================
# Protect the ETL endpoint with this key
ETL_API_KEY=your-secret-key-here

# ============================================
# Redis Cache (Optional - for performance)
# ============================================
# Get these from: https://upstash.com
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token-here

# ============================================
# Analytics (Optional)
# ============================================
# Vercel Analytics (free on Vercel)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=

# ============================================
# Feature Flags (Optional)
# ============================================
NEXT_PUBLIC_ENABLE_SEARCH=true
NEXT_PUBLIC_ENABLE_REAL_DATA=false
